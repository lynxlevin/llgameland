{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": [2, "always"],
        "comma-dangle": ["error", "only-multiline"]
    }
}
