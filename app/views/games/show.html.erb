<% breadcrumb :show_game, @game %>
<div class="game-wrapper">
  <button id="restart-game">RESTART</button>
  <div class="input">
    <input type="number" min="1" max="100" step="1" id="input">
    <p id="input-message"></p>
  </div>
  <table id="board"></table>
  <div class="game-info">
    <h2 id="info"></h2>
  </div>
</div>